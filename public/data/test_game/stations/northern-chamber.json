{
  "id": "northern-chamber",
  "type": "story",
  "description": "a dark chamber",
  "opens": [],
  "helpAudioFilenames": [
    "./audio/northern-chamber-help-1.mp3",
    "./audio/northern-chamber-help-2.mp3"
  ],

  "events": [
    {
      "action": "choiceBasedOnTags",
      "tags": ["lost"],

      "eventIfNotPresent": {
        "action": "playAudio",
        "wait": 0,
        "audioFilenames": ["./audio/northern-chamber.mp3"]
      },

      "eventIfPresent": {
        "action": "goToStation",
        "toStation": "northern-chamber-after-loosing"
      }
    },

    {
      "action": "playBackgroundAudio",
      "audioFilename": "./audio/startBackground.mp3",
      "loop": true,
      "cancelOnLeave": false,
      "wait": 0
    }
  ]
}
