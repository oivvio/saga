{
  "id": "northern-chamber",
  "type": "story",
  "description": "a dark chamber",
  "opens": [],

  "events": [
    {
      "action": "choiceBasedOnTags",
      "tags": ["lost"],

      "eventIfNotPresent": {
        "action": "playAudio",
        "wait": 0,
        "audioFilenames": ["./audio/northern-chamber_converted.WebM"]
      },

      "eventIfPresent": {
        "action": "goToStation",
        "toStation": "northern-chamber-after-loosing"
      }
    },

    {
      "action": "playBackgroundAudio",
      "audioFilename": "./audio/startBackground_converted.WebM",
      "loop": true,
      "cancelOnLeave": true,
      "wait": 0
    }
  ]
}
