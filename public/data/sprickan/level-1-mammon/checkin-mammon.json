{
  "id": "checkin-mammon",
  "type": "story",
  "description": "You have reached Mammon",
  "opens": [],
  "tags": ["mammon"],

  "events": [
    {
      "action": "choiceBasedOnTags",
      "tags": ["ringen"],
      "eventIfPresent": {
        
          "action": "playAudio",
          "wait": 0,
          "audioFilenames": ["./level-1-mammon/ring-ge-till-mammon-choice-square.mp3"],
    
      
    
          

            "then": {
              "action": "openStation",
              "toStation": ["ring-tillbaka-mammon-choice-circle, ring-tillbaka-mammon-choice-square"]
            }
          
        
      },
      "eventIfNotPresent": {
        "action": "playAudio",
        "wait": 0,
        "audioFilenames": ["./level-1-mammon/checkin-mammon.mp3"],

        "then": {
          "action": "openStation",
          "toStation": "qr-assemble"
        }
      }
    }
  ]
}
