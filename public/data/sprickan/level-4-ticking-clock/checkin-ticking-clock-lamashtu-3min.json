{
  "id": "checkin-ticking-clock-lamashtu-3min",
  "type": "story",
  "description": "Chose the path to slutstriden",
  "opens": ["checkin-slutstriden"],

  "events": [
    {
      "action": "startTimer",
      "name": "ticking-clock-lamashtu-3min",
      "time": 180,
      "then": {
        "action": "goToStation",
        "toStation": "checkin-sprickan-oppnas"
      }
    },

    {
      "action": "playAudio",
      "wait": 0,
      "audioFilenames": ["./level-4-ticking-clock/continuation-3min.mp3"],
      "then":
      {
        "action": "playBackgroundAudio",
        "audioFilename": "./level-4-ticking-clock/tiktok.mp3",
        "loop": true,
        "cancelOnLeave": true,
        "wait": 0
        ,

          "then":
        {
          "action": "playBackgroundAudio",
          "audioFilename": "./level-4-ticking-clock/ticking-clock-2min.mp3",
          "loop": false,
          "cancelOnLeave": true,
          "wait": 60
          ,

          "then":
        {
          "action": "playBackgroundAudio",
          "audioFilename": "./level-4-ticking-clock/ticking-clock-1min.mp3",
          "loop": false,
          "cancelOnLeave": true,
          "wait": 120
        }
      }
      }
      }
        
       
       
       
    
  ]
}
